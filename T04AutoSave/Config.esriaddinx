      </Commands>
      <Toolbars>
        <Toolbar id="HYZN-LGLONG519_T04AutoSave_工具" caption="自动保存工具" showInitially="true">
          <Items>
            <Item refID="HYZN-LGLONG519_T04AutoSave_AutoSave" />
            <Item refID="HYZN-LGLONG519_T04AutoSave_ToggleAutoSave" />
          </Items>
        </Toolbar>
      </Toolbars>
    </ArcMap>
  </AddIn>
</ESRI.Configuration>